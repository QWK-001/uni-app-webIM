<view><block tt:if="{{search_friend}}"><view class="search_input"><view><icon type="search" size="13"></icon><input placeholder="搜索" placeholder-style="color:#CFCFCF;line-height:20px;font-size:12px;" auto-focus/></view><text data-event-opts="{{[['tap',[['cancel',['$event']]]]]}}" bindtap="__e">取消</text></view></block><view class="contain"><view data-event-opts="{{[['tap',[['build_group',['$event']]]]]}}" class="build_group" bindtap="__e"><image src="../../static/images/chat-list.png"></image><text>新建群聊</text></view><block tt:for="{{groupList}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view class="groupList"><view class="nbr_header"></view><view data-event-opts="{{[['tap',[['into_info',['$event']]]]]}}" class="info" bindtap="__e"><image src="../../static/images/number.png" data-username="{{item.name}}"></image></view><view class="nbr_body" data-username="{{item.name}}" data-roomid="{{item.roomId}}" data-event-opts="{{[['tap',[['into_room',['$event']]]]]}}" bindtap="__e"><text data-username="{{item.name}}">{{item.name}}</text></view><view class="edit" data-username="{{item.name}}" data-roomid="{{item.roomId}}" data-event-opts="{{[['tap',[['edit_group',['$event']]]]]}}" bindtap="__e"><image src="../../static/images/cell_groups.png" data-username="{{item.name}}"></image></view></view></block></view><block tt:if="{{show_mask}}"><view data-event-opts="{{[['tap',[['close_mask',['$event']]]]]}}" class="mask" bindtap="__e"></view></block></view>