<view><block tt:if="{{search_btn}}"><view class="search"><view data-event-opts="{{[['tap',[['openSearch',['$event']]]]]}}" bindtap="__e"><icon type="search" size="13"></icon><text>请输入好友用户名</text></view></view></block><block tt:if="{{search_chats}}"><view class="search_input"><view><icon type="search" size="13"></icon><input placeholder="请输入好友用户名" placeholder-style="color:#CFCFCF;line-height:20px;font-size:12px;" auto-focus confirm-type="Search" type="text" data-event-opts="{{[['confirm',[['onSearch',['$event']]]],['input',[['onInput',['$event']]]]]}}" value="{{input_code}}" bindconfirm="__e" bindinput="__e"/><block tt:if="{{show_clear}}"><icon type="clear" size="13" data-event-opts="{{[['tap',[['clearInput',['$event']]]]]}}" catchtap="__e"></icon></block></view><text data-event-opts="{{[['tap',[['cancel',['$event']]]]]}}" bindtap="__e">取消</text></view></block><block tt:if="{{show_clear}}"><view class="address_bottom"><image src="../../static/images/theme@2x.png" data-username="1"></image><text>{{friend_name}}</text><button disabled="{{isdisable}}" data-event-opts="{{[['tap',[['add_friend',['$event']]]]]}}" bindtap="__e">发送申请</button></view></block><block tt:if="{{!show_clear}}"><view class="add_nobody">请输入好友的username,申请成为好友吧</view></block></view>